{
    "manifest_version":2,
    "name": "SpaceX Launcher",
    "description": "Quick launch for SpaceX",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.ico",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}